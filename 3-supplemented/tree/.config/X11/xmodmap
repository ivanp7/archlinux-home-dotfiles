clear lock
clear control
keycode 164 = Caps_Lock
keycode 105 = Caps_Lock
keycode 66 = Control_L
add lock = Caps_Lock
add control = Control_L Control_R

clear shift
keycode 107 = Shift_R Shift_R Shift_R Shift_R
add shift = Shift_L Shift_R

